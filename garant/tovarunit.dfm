  object tovarQuery: TmySQLQuery
    Database = dm.myBase
    SQL.Strings = (
      'select t.* ,g.name tiptovara from tovar t,tip g'
      'where'
      'g.id=t.tip '
      'and upper(t.name)  like upper(:name)'
      'and upper(t.serialnum) like upper(:sn)'
      'and  t.tip  like :tip'
      'order by t.tip,t.name')
    Left = 56
    Top = 120
    ParamData = <
      item
        DataType = ftString
        Name = 'name'
        ParamType = ptUnknown
        Value = '%'
      end
      item
        DataType = ftString
